{% extends 'base.html.twig' %}

{% block title %}Liste des espèces appartenant à la famille {{ familyName }} {% endblock %}
{% block body %}
    <h1>Liste des espèces appartenant à la famille {{ familyName }}</h1>
    <ul class="especesAnimal">
        {% for espece in species %}
            <li> {{ espece.libEspece  }} <span class="badge bg-secondary">{{ espece.descriptionEspece }} </span></li>
        {% endfor %}
    </ul>

{% endblock %}
